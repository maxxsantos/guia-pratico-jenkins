pipeline {
    agent any

    stages {
        stage('build docker image') {
            steps {
                sh 'echo "Executando o docker build"'
            }
        }
        stage('push docker image') {
            steps {
                sh 'echo "Executando o docker push"'
            }
        }
        stage('Deploy') {
            steps {
                sh 'echo "Executando o docker build"'
            }
        }
    }
}